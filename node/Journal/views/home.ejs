<%- include("./partials/header"); -%>

    <h1>Home</h1>

    <p>
        <%= startingContent %>
    </p>

    <% posts.forEach( function (e) { %>
        <h1>
            <%= e.title %>
        </h1>
        <p>
            <%= e.body.substring(0, 100) + "..." %>
            <a href="/posts/<%= e.title %>" class="extension-link">Read More </a>
        </p>
    <% }) %>

<%- include("./partials/footer"); -%>